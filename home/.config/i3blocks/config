[bat1]
command=echo -n "D"; cat /sys/class/power_supply/dock_battery/capacity
interval=10

[bat2]
command=echo -n "P"; cat /sys/class/power_supply/pad_battery/capacity
interval=10

[mic]
command=~/scripts/i3blocks/mic
interval=2

[volume]
command=~/scripts/i3blocks/volume
interval=2
signal=10

[time]
command=date +%m-%d\ %H:%M
interval=15

